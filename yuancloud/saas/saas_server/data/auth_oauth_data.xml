<?xml version="1.0"?>
<yuancloud>
    <data noupdate="1">
        <record id="saas_oauth_provider" model="auth.oauth.provider">
            <field name="name">SaaS</field>
            <field name="auth_endpoint">http://www.sswyuan.com/oauth2/auth</field>
            <field name="scope">userinfo</field>
            <field name="validation_endpoint">http://www.sswyuan.com/oauth2/tokeninfo</field>
            <field name="data_endpoint"></field>
            <field name="css_class">zocial yuancloud</field>
            <field name="body">via yuancloud</field>
            <field name="enabled" eval="True"/>
        </record>
    </data>
    <data>
        <delete id="auth_oauth.provider_yuancloud" model="auth.oauth.provider"/>
    </data>
</yuancloud>
